{
	"eslint.workingDirectories": [
		{
			"mode": "auto"
		}
	],
	"editor.defaultFormatter": "dbaeumer.vscode-eslint",
	"[typescriptreact]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[typescript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"svelte"
	],
	"eslint.enable": true,
	"[yaml]": {
		"editor.defaultFormatter": "redhat.vscode-yaml"
	}
}
